{
  "name": "vscode-6502-asm-syntax",
  "displayName": "6502汇编语法高亮",
  "description": "为 6502 汇编语言（.asm/.65s）文件提供语法高亮。支持常用指令、伪指令、寄存器和注释。",
  "version": "1.0.1",
  "publisher": "chengbin",
  "license": "MIT",
  "engines": {
    "vscode": ">=1.50.0"
  },
  "categories": ["Programming Languages"],
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/ZhenHena/vscode-6502-asm-syntax.git"
  },
  "bugs": {
    "url": "https://github.com/ZhenHena/vscode-6502-asm-syntax/issues"
  },
  "homepage": "https://github.com/ZhenHena/vscode-6502-asm-syntax",
  "keywords": ["6502", "assembly", "asm", "syntax", "highlight", "汇编", "语法高亮", "编程语言"],
  "main": "./extension.js",
  "activationEvents": [
    "onLanguage:6502-asm"
  ],
  "contributes": {
    "languages": [
      {
        "id": "6502-asm",
        "aliases": ["6502 Assembly", "6502 ASM"],
        "extensions": [".asm", ".65s"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "6502-asm",
        "scopeName": "source.6502-asm",
        "path": "./syntaxes/6502-asm.tmLanguage.json"
      }
    ]
  }
}
